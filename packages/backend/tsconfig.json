{
  "extends": "@darkobits/ts/tsconfig.json",
  "compilerOptions": {
    "rootDir": "./",
    "baseUrl": "src",
    "outDir": ".build",
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es2020",
    "lib": ["es2020"],
    "allowJs": true,
    "declaration": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "sourceMap": false
  }
}
