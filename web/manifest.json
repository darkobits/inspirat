{
  "manifest_version": 2,
  "name": "Inspirat",
  "description": "A minimalistic new tab experience for Chromium-based browsers.",
  "icons": {
    "16": "favicon-16.png",
    "48": "favicon-48.png",
    "128": "favicon-128.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'",
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
